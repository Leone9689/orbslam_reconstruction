<!-- This file shows the most important parameters in their default settings,
     to make them easily available for beginners.

     The openni driver is started automatically -->
<launch>
  <!--include file="$(find openni2_launch)/launch/openni2.launch"/ -->
  <node pkg="orbslam2_ros" type="RGBD" name="orbslam2_ros" args="$(find orbslam2_ros)/Data/ORBvoc.bin $(find orbslam2_ros)/Data/TUM1.yaml" output="screen" > 
  <remap from="/vslam/rgb/image_raw" to="/camera/rgb/image_color"/>
  <remap from="/vslam/depth_registered/image_raw" to="/camera/depth/image"/>
  <remap from="/vslam/depth/camera_info" to="/camera/depth/camera_info"/>
  <remap from="/imu" to="torso_lift_imu/data"/>
 
  <param name="use_pango_viewer" type="bool" value="true" />
  <param name="use_imu_data" type="bool" value="false" />
  </node>
</launch>
